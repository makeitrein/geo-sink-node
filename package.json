    "pg": "^8.11.3",
    "pg-cursor": "^2.10.3",
    "prom-client": "latest",
    "tslog": "latest",
    "zapatos": "^6.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tsconfig/recommended": "latest",
    "@types/node": "latest",
    "dotenv-cli": "^7.3.0",
